{
  "compilerOptions": {
    //    "noUnusedLocals": false,
    //    "noUnusedParameters": false,
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ES2017",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts"
    //    ".next/types/**/*.ts"
    //    "build/types/**/*.ts"
    //    "build/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
